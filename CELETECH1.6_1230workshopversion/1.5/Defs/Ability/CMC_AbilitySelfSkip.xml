<Defs>
  <AbilityDef>
    <defName>CMC_SelfSkip</defName>
    <label>teleport(portal beam)</label>
    <description>After sending the signal from the blade, portal beam equipped on a nearby star destroyer skips the target to a desired position not too far from its starting point.</description>
    <iconPath>UI/UI_SelfSkip</iconPath>
    <writeCombatLog>False</writeCombatLog>
    <showPsycastEffects>False</showPsycastEffects>
    <hotKey>Misc9</hotKey>
	<cooldownTicksRange>90</cooldownTicksRange>
    <aiCanUse>true</aiCanUse>
    <verbProperties>
	  <verbClass>Verb_CastAbility</verbClass>
      <warmupTime>0.1</warmupTime>
      <range>11.9</range>
      <targetParams>
		<canTargetPawns>true</canTargetPawns>
		<canTargetLocations>true</canTargetLocations>
      </targetParams>
	  <requireLineOfSight>false</requireLineOfSight>
    </verbProperties>
    <comps>
      <li Class = "TOT_DLL_test.CompProperties_AbilitySelfSkip">
        <compClass>TOT_DLL_test.CompAbilityEffect_SelfSkip</compClass>
        <destination>Selected</destination>
        <requiresLineOfSight>False</requiresLineOfSight>
      </li>
    </comps>
  </AbilityDef>
</Defs>