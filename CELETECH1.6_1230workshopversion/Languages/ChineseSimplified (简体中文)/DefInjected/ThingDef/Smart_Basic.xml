  <W85X_Thunderer.tools.stock.label>枪托</W85X_Thunderer.tools.stock.label>
  <!-- EN: barrel -->
  <W85X_Thunderer.tools.barrel.label>枪管</W85X_Thunderer.tools.barrel.label>
