<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingCategoryDef>
    <defName>AmmoCategory_CMC_Ammo_45x250mm</defName>
    <label>45x250mm Electromagnetic</label>
    <parent>AmmoShells</parent>
    <iconPath>Things/Ammo/45x250mmUI</iconPath>
  </ThingCategoryDef>
  <CombatExtended.AmmoSetDef>
    <defName>AmmoSet_CMC_Ammo_45x250mm</defName>
    <label>45x250mm Electromagnetic</label>
    <ammoTypes>
      <Ammo_CMC_Ammo_45x250mm_HE>Bullet_CMC_Ammo_45x250mm_HE</Ammo_CMC_Ammo_45x250mm_HE>
      <Ammo_CMC_Ammo_45x250mm_APDS-EMP>Bullet_CMC_Ammo_45x250mm_APDS-EMP</Ammo_CMC_Ammo_45x250mm_APDS-EMP>
    </ammoTypes>
  </CombatExtended.AmmoSetDef>
  <ThingDef Class="CombatExtended.AmmoDef" Name="Ammo_CMC_Ammo_45x250mm_Base" ParentName="HeavyAmmoBase" Abstract="True">
    <description>High caliber EM-shell</description>
    <statBases>
      <Mass>1</Mass>
      <Bulk>1.2</Bulk>
    </statBases>
    <tradeTags>
      <li>CE_AutoEnableTrade</li>
      <li>CE_AutoEnableCrafting</li>
    </tradeTags>
    <thingCategories>
      <li>AmmoCategory_CMC_Ammo_45x250mm</li>
    </thingCategories>
  </ThingDef>
  <ThingDef Name="Bullet_CMC_Ammo_45x250mm_Base" ParentName="BaseBulletCE" Abstract="True">
    <graphicData>
      <texPath>Things/Projectile/Bullet_QLUX</texPath>
      <graphicClass>Verse.Graphic_Single</graphicClass>
    </graphicData>
    <projectile Class="CombatExtended.ProjectilePropertiesCE">
      <speed>240</speed>
      <damageDef>Bullet</damageDef>
      <dropsCasings>true</dropsCasings>
    </projectile>
  </ThingDef>
  <ThingDef Class="CombatExtended.AmmoDef" ParentName="Ammo_CMC_Ammo_45x250mm_Base">
    <defName>Ammo_CMC_Ammo_45x250mm_HE</defName>
    <label>CMC_Ammo_45x250mm HE</label>
    <graphicData>
      <texPath>Things/Ammo/45x250mm_HE</texPath>
      <graphicClass>Verse.Graphic_StackCount</graphicClass>
    </graphicData>
    <ammoClass>GrenadeHE</ammoClass>
    <cookOffProjectile>Bullet_CMC_Ammo_45x250mm_HE</cookOffProjectile>
  </ThingDef>
  <ThingDef ParentName="Bullet_CMC_Ammo_45x250mm_Base">
    <defName>Bullet_CMC_Ammo_45x250mm_HE</defName>
    <projectile Class="CombatExtended.ProjectilePropertiesCE">
      <damageAmountBase>40</damageAmountBase>
      <damageDef>Bomb</damageDef>
      <explosionRadius>2.8</explosionRadius>
      <suppressionFactor>3</suppressionFactor>
    </projectile>
  </ThingDef>
  <RecipeDef ParentName="AmmoRecipeBase">
    <defName>MakeAmmo_CMC_Ammo_45x250mm_HE</defName>
    <label>make CMC_Ammo_45x250mm HE cartridge x500</label>
    <description>Craft 100 CMC_Ammo_45x250mm HE cartridges.</description>
    <jobString>Making CMC_Ammo_45x250mm HE cartridges.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>FSX</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentSpacer</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>75</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>FSX</li>
        <li>ComponentSpacer</li>
        <li>Plasteel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_CMC_Ammo_45x250mm_HE>100</Ammo_CMC_Ammo_45x250mm_HE>
    </products>
    <workAmount>4000</workAmount>
  </RecipeDef>
  <ThingDef Class="CombatExtended.AmmoDef" ParentName="Ammo_CMC_Ammo_45x250mm_Base">
    <defName>Ammo_CMC_Ammo_45x250mm_APDS-EMP</defName>
    <label>CMC_Ammo_45x250mm APDS-EMP</label>
    <graphicData>
      <texPath>Things/Ammo/45x250mm_EMP</texPath>
      <graphicClass>Verse.Graphic_StackCount</graphicClass>
    </graphicData>
    <ammoClass>Sabot</ammoClass>
    <cookOffProjectile>Bullet_CMC_Ammo_45x250mm_APDS-EMP</cookOffProjectile>
  </ThingDef>
  <ThingDef ParentName="Bullet_CMC_Ammo_45x250mm_Base">
    <defName>Bullet_CMC_Ammo_45x250mm_APDS-EMP</defName>
    <projectile Class="CombatExtended.ProjectilePropertiesCE">
      <damageAmountBase>35</damageAmountBase>
      <damageDef>BulletAoE_Void</damageDef>
      <armorPenetrationBlunt>230</armorPenetrationBlunt>
      <armorPenetrationSharp>120</armorPenetrationSharp>
      <secondaryDamage>
        <li>
          <def>EMP</def>
          <amount>15</amount>
        </li>
      </secondaryDamage>
    </projectile>
    <comps>
      <li Class="CombatExtended.CompProperties_ExplosiveCE">
        <damageAmountBase>25</damageAmountBase>
        <explosiveDamageType>EMP</explosiveDamageType>
        <explosiveRadius>1.9</explosiveRadius>
      </li>
    </comps>
  </ThingDef>
  <RecipeDef ParentName="AmmoRecipeBase">
    <defName>MakeAmmo_CMC_Ammo_45x250mm_APDS-EMP</defName>
    <label>make CMC_Ammo_45x250mm EEJ cartridge x500</label>
    <description>Craft 500 CMC_Ammo_45x250mm EEJ cartridges.</description>
    <jobString>Making CMC_Ammo_45x250mm EEJ cartridges.</jobString>
    <ingredients />
    <fixedIngredientFilter>
      <thingDefs />
    </fixedIngredientFilter>
    <products>
      <Ammo_CMC_Ammo_45x250mm_APDS-EMP>500</Ammo_CMC_Ammo_45x250mm_APDS-EMP>
    </products>
    <workAmount>2000</workAmount>
  </RecipeDef>
</Defs>