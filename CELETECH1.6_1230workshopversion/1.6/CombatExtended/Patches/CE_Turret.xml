<Patch>
	<!--CMCcannon-->
	<Operation Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName="CMCcannon"]/thingClass</xpath>
        <value>
            <thingClass>CombatExtended.Building_TurretGunCE</thingClass>
        </value>
    </Operation>
    <Operation Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[defName="CMCcannon"]/statBases</xpath>
        <value>
            <Bulk>1500</Bulk>
        </value>
    </Operation>
	<Operation Class="PatchOperationAddModExtension">
		<xpath>/Defs/ThingDef[defName="CMCcannon"]</xpath>
        <value>
            <li Class="CombatExtended.NonSnapTurretExtension">
			<speed>1.25</speed>
			<angleWeightMultiplier>1</angleWeightMultiplier>
			</li>
		</value>
    </Operation>
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName="CMCcannon"]/statBases/ShootingAccuracyTurret</xpath>
        <value>
            <ShootingAccuracyTurret>1.00</ShootingAccuracyTurret>
            <AimingAccuracy>0.50</AimingAccuracy>
        </value>
    </Operation>
    <Operation Class="PatchOperationRemove">
        <xpath>/Defs/ThingDef[defName="CMCcannon"]/comps/li[@Class="CompProperties_Refuelable"]</xpath>
    </Operation>
	
    <Operation Class="CombatExtended.PatchOperationMakeGunCECompatible">
        <defName>Gun_CMCcannon</defName>
        <statBases>
            <Bulk>120</Bulk>
            <SightsEfficiency>1.9</SightsEfficiency>
            <ShotSpread>0</ShotSpread>
            <SwayFactor>0</SwayFactor>
        </statBases>
        <Properties>
            <verbClass>CombatExtended.Verb_ShootCE</verbClass>
            <defaultProjectile>Bullet_CMC_30x114mm_APDS</defaultProjectile>
            <warmupTime>0.5</warmupTime>
            <recoilAmount>0</recoilAmount>
            <range>90</range>
            <ticksBetweenBurstShots>3</ticksBetweenBurstShots>
            <burstShotCount>10</burstShotCount>
            <soundCast>Sound_Cannon</soundCast>
            <soundCastTail>GunTail_Light</soundCastTail>
            <muzzleFlashScale>30</muzzleFlashScale>
            <targetParams>
				<canTargetLocations>true</canTargetLocations>
			</targetParams>
        </Properties>
        <AmmoUser>
            <magazineSize>1000</magazineSize>
            <reloadTime>12</reloadTime>
            <ammoSet>AmmoSet_CMC_30x114mm</ammoSet>
        </AmmoUser>
        <FireModes>
            <aiAimMode>AimedShot</aiAimMode>
            <noSnapshot>true</noSnapshot>
            <aiUseBurstMode>true</aiUseBurstMode>
        </FireModes>
    </Operation>
	
