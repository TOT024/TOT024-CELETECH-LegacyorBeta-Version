	<ThingDef ParentName="BaseWeaponTurret">
		<defName>CMCUAV_LaserPulse</defName>
		<label>Escort Floating Laser</label>
		<description>A miniaturized pulse laser focusing array compatible with armed floating platforms.</description>
		<tradeability>None</tradeability>
		<destroyOnDrop>true</destroyOnDrop>
		<graphicData>
			<texPath>Things/CMC_Void</texPath>
