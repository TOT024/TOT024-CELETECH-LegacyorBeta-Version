	<ThingDef  MayRequire="ludeon.rimworld.royalty" ParentName="ApparelBase">
        <defName>CMC_rocketpack</defName>
        <label>BB-01Z "Chaofeng" Pulse Jetpack</label>
        <description>CMC individual mobility system. Equipped with vectored pulse engines enabling rapid vertical ascent, short-range bursts, and tactical jumps. Significantly enhances battlefield maneuverability and positioning options.</description>
        <techLevel>Spacer</techLevel>
        <comps>
            <li Class="CompProperties_ApparelReloadable">
                <maxCharges>8</maxCharges>
                <ammoDef>Chemfuel</ammoDef>
                <ammoCountPerCharge>20</ammoCountPerCharge>
                <baseReloadTicks>60</baseReloadTicks>
                <soundReload>Standard_Reload</soundReload>
                <hotKey>Misc4</hotKey>
                <chargeNoun>jump</chargeNoun>
                <displayGizmoWhileUndrafted>false</displayGizmoWhileUndrafted>
            </li>
        </comps>
        <verbs>
            <li>
                <verbClass>Verb_Jump</verbClass>
                <label>jump</label>
                <violent>false</violent>
                <hasStandardCommand>true</hasStandardCommand>
                <onlyManualCast>True</onlyManualCast>
                <forceNormalTimeSpeed>false</forceNormalTimeSpeed>
                <warmupTime>0</warmupTime>
                <warmupEffecter>JumpWarmupEffect</warmupEffecter>
                <requireLineOfSight>true</requireLineOfSight>
                <flightEffecterDef>JumpFlightEffect</flightEffecterDef>
                <soundLanding>JumpPackLand</soundLanding>
                <targetParams>
                    <canTargetLocations>true</canTargetLocations>
                    <canTargetPawns>false</canTargetPawns>
                    <canTargetBuildings>false</canTargetBuildings>
                </targetParams>
            </li>
        </verbs>
        <recipeMaker>
            <unfinishedThingDef>UnfinishedPack</unfinishedThingDef>
            <researchPrerequisites>
                <li>JumpPack</li>
                <li>CMCarmorII</li>
            </researchPrerequisites>
            <recipeUsers Inherit="False">
                <li>CMC_FacBench</li>
            </recipeUsers>
            <useIngredientsForColor>false</useIngredientsForColor>
            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
            <workSkill>Crafting</workSkill>
            <effectWorking>Smith</effectWorking>
            <soundWorking>Recipe_Machining</soundWorking>
            <skillRequirements>
                <Crafting>4</Crafting>
            </skillRequirements>
            <displayPriority>330</displayPriority>
        </recipeMaker>
        <costList>
            <Steel>200</Steel>
            <ComponentIndustrial>10</ComponentIndustrial>
            <ComponentSpacer>2</ComponentSpacer>
        </costList>
        <graphicData>
            <texPath>Things/Apparel/Back_Aux/CMC_ERP_Dispay</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <tickerType>Normal</tickerType>
        <statBases>
            <ArmorRating_Sharp>0.32</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.5</ArmorRating_Blunt>
            <MaxHitPoints>200</MaxHitPoints>
            <WorkToMake>9000</WorkToMake>
            <Mass>4.3</Mass>
            <Flammability>0.4</Flammability>
            <EquipDelay>2</EquipDelay>
            <JumpRange>23.9</JumpRange>
            <CarryingCapacity>25</CarryingCapacity>
        </statBases>
        <equippedStatOffsets>
            <MoveSpeed>-0.4</MoveSpeed>
            <VacuumSpeedMultiplier MayRequire="kentington.saveourship2">6</VacuumSpeedMultiplier>
        </equippedStatOffsets>
        <thingSetMakerTags>
            <li>RewardStandardQualitySuper</li>
        </thingSetMakerTags>
        <thingCategories>
            <li>ApparelUtility</li>
        </thingCategories>
        <apparel>
            <countsAsClothingForNudity>false</countsAsClothingForNudity>
            <scoreOffset>4</scoreOffset>
            <wornGraphicPath>Things/Apparel/Back_Aux/CMC_ERP</wornGraphicPath>
            <useWornGraphicMask>true</useWornGraphicMask>
            <wornGraphicData>
                <renderUtilityAsPack>true</renderUtilityAsPack>
                <east>
                    <offset>(-0.32,0.1)</offset>
                    <thin>
                        <offset>(0.05,0)</offset>
                    </thin>
                    <hulk>
                        <offset>(-0.1,0)</offset>
                    </hulk>
                    <fat>
                        <offset>(-0.15,0)</offset>
                    </fat>
                </east>
                <west>
                    <offset>(0.32,0.1)</offset>
                    <thin>
                        <offset>(-0.05,0)</offset>
                    </thin>
                    <hulk>
                        <offset>(0.1,0)</offset>
                    </hulk>
                    <fat>
                        <offset>(0.15,0)</offset>
                    </fat>
                </west>
                <male>
                    <scale>(0.85,0.85)</scale>
                </male>
                <female>
                    <scale>(0.85,0.85)</scale>
                </female>
                <thin>
                    <scale>(0.85,0.85)</scale>
                </thin>
                <hulk>
                    <scale>(1,1)</scale>
                </hulk>
                <fat>
                    <scale>(1,1)</scale>
                </fat>
            </wornGraphicData>
            <careIfWornByCorpse>false</careIfWornByCorpse>
            <careIfDamaged>false</careIfDamaged>
            <wearPerDay>0</wearPerDay>
            <bodyPartGroups>
                <li>Torso</li>
            </bodyPartGroups>
            <layers>
                <li>Belt</li>
            </layers>
            <tags>
                <li>PackJump</li>
            </tags>
        </apparel>
        <tradeTags>
            <li>ExoticMisc</li>
            <li>Clothing</li>
        </tradeTags>
        <allowedArchonexusCount>1</allowedArchonexusCount>
    </ThingDef>
	
